<h2>Editing <%= @contact.full_name -%></h2>

<%= form_for @contact do |f| %>
  <%= render "shared/error_messages", :target => @contact %>
  <dl>
    <%= render :partial => 'form' -%>
    <dd><%= f.submit "Update", :class => "button" -%>&nbsp;&nbsp;<%= link_to 'Cancel', @contact %></dd>
  </dl>
<% end -%>
